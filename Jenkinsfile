node('node') {
   stage('Stage  1') {
     sleep 3
     print 'stage 1, step  1'
     sleep 3
     print 'stage 1, step - 2'
     sleep 4
     print 'stage 1, step 3'
   }
   stage('Stage  - 2') {
     sleep 3
     print 'stage 2, step 1'
     sleep 3
     print 'stage 2, step 2'
     sleep 4
     print 'stage 2, step 3'
   }
}
